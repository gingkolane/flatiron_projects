<p id="notice"><%= notice %></p>

<h1>Studies</h1>

<table>
  <thead>
    <tr>
      <th>Nct id</th>
      <th>Brief title</th>
      <th>Brief summary</th>
      <th>Overall status</th>
      <th>Start date</th>
      <th>Start date type</th>
      <th>Completion date</th>
      <th>Completion date type</th>
      <th>Phase</th>
      <th>Study type</th>
      <th>Enrollment type</th>
      <th>Enrollment</th>
      <th>Criteria</th>
      <th>Gender</th>
      <th>Minimum age</th>
      <th>Maximum age</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @studies.each do |study| %>
      <tr>
        <td><%= study.NCT_ID %></td>
        <td><%= study.BRIEF_TITLE %></td>
        <td><%= study.BRIEF_SUMMARY %></td>
        <td><%= study.OVERALL_STATUS %></td>
        <td><%= study.START_DATE %></td>
        <td><%= study.START_DATE_TYPE %></td>
        <td><%= study.COMPLETION_DATE %></td>
        <td><%= study.COMPLETION_DATE_TYPE %></td>
        <td><%= study.PHASE %></td>
        <td><%= study.STUDY_TYPE %></td>
        <td><%= study.ENROLLMENT_TYPE %></td>
        <td><%= study.ENROLLMENT %></td>
        <td><%= study.CRITERIA %></td>
        <td><%= study.GENDER %></td>
        <td><%= study.MINIMUM_AGE %></td>
        <td><%= study.MAXIMUM_AGE %></td>
        <td><%= study.URL %></td>
        <td><%= link_to 'Show', study %></td>
        <td><%= link_to 'Edit', edit_study_path(study) %></td>
        <td><%= link_to 'Destroy', study, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study', new_study_path %>
