<p id="notice"><%= notice %></p>

<h1>Sponsors</h1>

<table>
  <thead>
    <tr>
      <th>Agency</th>
      <th>Sponsor type</th>
      <th>Agency class</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sponsors.each do |sponsor| %>
      <tr>
        <td><%= link_to sponsor.AGENCY, sponsor %></td>
        <td><%= sponsor.SPONSOR_TYPE %></td>
        <td><%= sponsor.AGENCY_CLASS %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sponsor', new_sponsor_path %>
